<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
 
	<application>
		<el-resolver>
    		    org.springframework.web.jsf.el.SpringBeanFacesELResolver
		</el-resolver>
  	</application>
  <navigation-rule>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>

<navigation-rule>
        <navigation-case>
            <from-outcome>apresentaNotas</from-outcome>
            <to-view-id>/app/notasAluno.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>
<navigation-rule>
        <navigation-case>
            <from-outcome>apresentaNotasAtendente</from-outcome>
            <to-view-id>/app/notasAlunoAtendente.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>
<navigation-rule>
        <navigation-case>
            <from-outcome>apresentaNotasCoordenador</from-outcome>
            <to-view-id>/app/notasAlunoCoordenador.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>
</faces-config>